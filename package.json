{
  "name": "etfcurator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "import-excel": "tsx scripts/import-excel-data.ts",
    "fmp-enrich": "tsx scripts/fmp-integration.ts",
    "data-import-complete": "npm run import-excel && npm run fmp-enrich -- --limit 100",
    "diagnose": "tsx scripts/diagnose-data.ts",
    "migrate:data-sources": "npx tsx scripts/migrate-data-sources.ts",
    "test:data-sources": "npx tsx scripts/test-data-sources.ts",
    "dev:optimized": "node scripts/start-dev.js",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset --force && prisma generate",
    "db:seed": "node -r ts-node/register prisma/seed.ts",
    "update-etf-data": "tsx scripts/update_etf_data.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
